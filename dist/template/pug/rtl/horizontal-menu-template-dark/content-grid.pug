					// Horizontal alignment
					section#horizontal-alignment.row
						.col-md-12
							.card
								.card-header
									h4.card-title Horizontal alignment
								.card-content
									.card-body
										.bd-example-row
											.bd-example
												.container
													.row.justify-content-start
														.col-4.
															
